
// Step 1
export const romanNumeralConverter = number => {
  return "I";
}

// Step 2
export const romanNumeralConverter = number => {
  switch(number) {
    case 1:
      return "I";
    case 2:
      return "II";
  }
}

// Step 3
export const romanNumeralConverter = number => {
  switch(number) {
    case 1:
      return "I";
    case 2:
      return "II";
    default:
      return "Case Not Covered";
  }
}